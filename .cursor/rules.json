{
    "exclude": [
      "**/node_modules/**",
      "**/dist/**",
      "**/build/**",
      "**/.next/**"
    ],
    "ignoreBuildCommands": [
      "npm run build",
      "npm build",
      "yarn build"
    ],
    "verification": {
      "skipBuild": true,
      "onlyVerifyChangedFiles": true
    }
  }